{
  "extends": "@workspace/typescript-config/react-library.json",
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
        "@workspace/shared/types/*": ["./src/types/*"],
      "@workspace/shared/lib/*": ["./src/lib/*"],
      "@workspace/database/*": ["../../packages/database/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.css", "**/*.mjs", "**/*.js"],
  "exclude": ["node_modules", "dist"]
}
